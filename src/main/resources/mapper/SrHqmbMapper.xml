<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ctsi.ssdc.project.mapper.SrHqmbMapper">


    <select id="getSrHqmbList" parameterType="com.ctsi.ssdc.project.domain.SrHqmb" resultType="com.ctsi.ssdc.project.domain.SrHqmb">
        select
            id as id,
            module_name as moduleName,
            status as status,
            create_time as createTime
        from
            sr_hqmb
        where
            1=1
        <if test="moduleName!=null and moduleName!=''">
           and module_name = #{moduleName}
        </if>
        order by create_time desc
    </select>
</mapper>