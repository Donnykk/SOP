<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ctsi.ssdc.project.mapper.SrRegionsInfoMapper">

    <select id="getPage" parameterType="com.ctsi.ssdc.project.domain.SrRegionsInfo" resultType="com.ctsi.ssdc.project.domain.SrRegionsInfo">
        select * from sr_regions_info
        where
        1=1
        <if test="indexCode !=null and indexCode !=''">
            and index_code = #{indexCode}
        </if>
        <if test="name !=null and name !=''">
            and name = #{name}
        </if>

    </select>

</mapper>